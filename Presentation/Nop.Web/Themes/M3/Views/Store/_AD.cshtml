@model List<Maticsoft.Model.Shop.Supplier.SupplierAD>
@if (Model != null && Model.Count>0)
{
 <div id="full-screen-slider">
     <ul id="slides">
         @foreach (Maticsoft.Model.Shop.Supplier.SupplierAD item in Model)
         {
             <li >
                 <a href="@item.NavigateUrl" target="_blank" >
                     <img src="@item.FileUrl" alt="@item.Name" title="@item.Name" />
                 </a>
             </li> 
         }   
     </ul>
</div>
}
