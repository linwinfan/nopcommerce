@model List<Maticsoft.Model.Shop.Products.CategoryInfo>
@if (Model != null && Model.Count > 0)
{
   
        for (int i = 0; i < Model.Count; i++)
        {
            <dd>
    <a href="@(ViewBag.BasePath)product/@Model[i].CategoryId" target="_blank" title="@Model[i].Name" >@Maticsoft.Common.StringPlus.SubString(Model[i].Name, 12, "...")</a>
    </dd>
        }
   }