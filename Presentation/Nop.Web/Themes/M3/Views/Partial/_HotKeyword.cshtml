@model List<Maticsoft.Model.Shop.Products.HotKeyword>
 @if (Model != null && Model.Count > 0)
 {
     <ul class="hotSearch">
			@foreach (var item in Model)
     {
         <li><a href="@(ViewBag.BasePath)search/@item.CategoryId/0/default/0-0/@item.Keywords" target="_blank">@item.Keywords</a></li>
      
     }  
            
            </ul>
     
 }
   
                  