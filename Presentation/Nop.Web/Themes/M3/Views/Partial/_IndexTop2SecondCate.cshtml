@model List<Maticsoft.Model.Shop.Products.CategoryInfo>
@if (Model != null && Model.Count > 0)
{
    <p class="c-category-list limit-24">
        @for (int i = 0; i < Model.Count; i++)
        {
            <a href="@(ViewBag.BasePath)product/@Model[i].CategoryId" target="_blank" title="@Model[i].Name" id="s-goods-category-289-1">@Maticsoft.Common.StringPlus.SubString(Model[i].Name, 4, "...")</a>
        }
    </p>}