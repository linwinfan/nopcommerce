@model List<Maticsoft.Model.Shop.Products.BrandInfo>
@if (Model != null && Model.Count > 1)
{         
    <div class="brand-section">  
    <div class="f1-hrand_title fclr" style="padding: 0px 10px;width: 95%;">
         <h3 style="color: #444444;display: inline;float: left;font-size: 16px;">@Model[Model.Count - 1].BrandName</h3>
     </div>
        <div class="f1-hrand_content">
            <div class="f1-hrand_list" style="width: 100%;margin-bottom: 15px;">
                <ul style="clear: both;width: 1034px;overflow: hidden;margin-bottom: 10px;">
                    @for (int i = 0; i < Model.Count - 1; i++)
                    {
                        <li style="background-color: #FFFFFF;float: left;height: 80px;margin: 10px 32px 10px 0px;text-align: center;width: 140px;">
                            <a href="@(ViewBag.BasePath)Product/0/@Model[i].BrandId" class="brand-logo">
                                <img width="115" height="50" title="@Model[i].BrandName" src="@Model[i].Logo" alt="@Model[i].BrandName" >
                                <div>
                                    <span>@Model[i].BrandName</span>
                                </div>
                            </a>
                        </li>
                    }
                </ul>
                <div style="clear: both;">
                </div>
            </div>
        </div>
    </div>
}
