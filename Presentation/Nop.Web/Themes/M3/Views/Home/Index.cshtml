        <div class="womenWearCont1">
            @Html.Action("AdDetail", "Partial", new { id = 77, ViewName = "_Index_Cate_Ad" })
            @Html.Action("CategoryList", "Partial", new { Cid = 2, Top = 10, ViewName = "_IndexCate" })
        </div>
        @Html.Action("ProductList", new { Cid = 2, Top = 8 })
        @Html.Action("BrandList", "Product", new { Cid = 2, Top = 9, viewName = "_CateBrand" })
    </div>
</div>
<!--womenWearWrap精品女装结束-->
