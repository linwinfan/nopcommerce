@{
    Layout = "/Areas/Shop/Themes/M3/Views/Shared/_BaseLayout.cshtml";
}
<link rel="stylesheet" type="text/css" href="/Areas/Shop/Themes/M3/Content/styles/style.css" />
<script type="text/javascript" src="/Areas/Shop/Themes/M3/Content/scripts/jquery.soChange-min.js"></script>
<link rel="stylesheet" type="text/css" href="/Areas/Shop/Themes/M3/Content/styles/zzsc.css" />
@if (Maticsoft.Web.MvcApplication.HasArea(Maticsoft.Web.AreaRoute.MShop))
{
    <script type="text/javascript">
        try {
            if ((navigator.userAgent.match(/(iPhone|iPod|Android|ios|iPad)/i))) {
                location.replace('/m');
            }
        } catch (e) { }
    </script>
}
<!--banner焦点图脚本结束-->
<div class="newBannerWrap">
    <div class="new_banner">
        <div class="rslides " style="height: 480px;">
            @Html.Action("AdDetail", "Partial", new { id = 66, ViewName = "_Ad_Banner" })
        </div>
        <ul class="rslides_tabs ">
            @Html.Action("AdDetail", "Partial", new { id = 66, ViewName = "_Ad_Banner_Cons" })
        </ul>
    </div>
</div>
<!--banner焦点图结束-->
<!--smSlider结束-->
<!--flashSale限时抢购开始-->
@Html.Action("CountDown", "ProSales", new { viewName = "_CountDownIndex" })
<!--flashSale限时抢购结束-->
<!--brandRecommendation品牌推荐开始-->
@Html.Action("ProductRec", "Partial", new { Type = Maticsoft.Model.Shop.Products.ProductRecType.Cheap, Top = 5, ViewName = "_ProductIndexCheap" })
<!--brandRecommendation品牌推荐结束-->
<!--womenWearWrap精品女装开始-->
